# Test rollup for positive typecheck modules
# Runs selected tests in test/typecheck/

import test::typecheck::simple as simple
import test::typecheck::primitives as primitives
import test::typecheck::match as match
import test::typecheck::fold_inline as fold_inline
import test::typecheck::mono as mono
import test::typecheck::type_family as type_family
import test::typecheck::typeclass_parse as typeclass_parse
import test::typecheck::typeclass_resolution as typeclass_resolution
import test::typecheck::typeclass_variable as typeclass_variable
import test::typecheck::opaque_import as opaque_import

module test::typecheck contains
  define transparent main as compute
    sequence
      simple::main
      primitives::main
      match::main
      fold_inline::main
      mono::main
      type_family::main
      typeclass_parse::main
      typeclass_resolution::main
      typeclass_variable::main
      opaque_import::main
    end
  end
end
