import prelude as P
import effects as Effects

open Effects exposing Effects::io end

module test::typecheck::effects contains
  define transparent io-action as
    of-type
      (compute return Unit::tt end)
      (computation Effects::io Unit)

  define transparent accept-io as
    function c (computation Effects::io Unit) returns computation Effects::io Unit value
      c
    end

  define transparent main as
    P::map-comp Unit Natural
      (function ignored Unit returns Natural value
        0
      end)
      (accept-io io-action)
end
