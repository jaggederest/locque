# Test that an imported module can reference its own values

import prelude as P

module helper_values contains
  define transparent helper-value as value
    function x of-type Nat produce
      P.add-nat x 10

  define transparent use-helper as value
    function y of-type Nat produce
      helper-value y  # Self-reference to helper-value

  define transparent main as computation
    return P.tt
end
