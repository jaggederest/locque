module LTypeCheckMono contains
  define transparent five as value 5

  define transparent ten as value add-nat-prim 5 5

  define transparent hello as value "hello world"

  define transparent combined as value concat-string-prim "hello" " world"

  define transparent is-equal as value eq-nat-prim 5 10

  define transparent main as computation
    print-prim "Testing type checker"
end
