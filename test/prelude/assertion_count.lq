# Test: Assertion counting
# This file tests that the assertion counter correctly counts all assertions
# Expected: 5 assertions

import assert as A

module test::prelude::assertion_count contains
  define transparent test-five-assertions as compute
    sequence
      (A::assert-eq-nat 1 1)
      (A::assert-eq-nat 2 2)
      (A::assert-eq-nat 3 3)
      (A::assert-eq-string "a" "a")
      (A::assert-eq-string "b" "b")
    end
  end

  define transparent main as compute
    perform test-five-assertions
  end
end
