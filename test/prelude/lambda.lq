# Migrated from ../02_lambda.lq

import prelude as P
import assert as A

module LLambdaTests contains

  define transparent id as value
    function x of-type String produce x

  define transparent apply-id as computation
    bind id "hi" as res then
    bind perform io (A.assert-eq-string res "hi") as _ then
    return P.tt

  define transparent main as computation
    bind apply-id as _ then
    return P.tt
end
