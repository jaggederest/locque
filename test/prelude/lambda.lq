# Migrated from ../02_lambda.lq

import prelude as P
import assert as A
import typeclass::equality as Eq
import string as S

module test::prelude::lambda contains
  define transparent id as
    function x String returns String value
      x
    end

  define transparent apply-id as compute
    bind res from return (id "hi") then
      perform (P::sequence-unit
        [
          (A::assert-eq String res "hi")
        ])
    end
  end

  define transparent main as compute
    perform (P::sequence-unit
      [
        apply-id
      ])
  end
end
