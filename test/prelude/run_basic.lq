import prelude as P
# Test: smyth run - basic execution
# This file tests that `smyth run` can execute a simple program

import assert as A
import typeclass::equality as Eq
import string as S

module test::prelude::run_basic contains
  define transparent test-run-basic as compute
    perform (P::sequence-unit
      [
        (A::assert-eq Natural 1 1)
      , (A::assert-eq String "hello" "hello")
      ])
  end

  define transparent main as compute
    perform test-run-basic
  end
end
