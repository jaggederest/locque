# Migrated from ../09_comparison.lq

import Prelude as P
import Assert as A
import Comparison as C

module ComparisonTests contains

  define transparent test-between as computation
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 20 15) "yes" "no") "yes") then
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 20 10) "yes" "no") "yes") then
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 20 20) "yes" "no") "yes") then
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 20 5) "yes" "no") "no") then
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 20 25) "yes" "no") "no") then
    bind _ <- perform io (A.assert-eq-string (P.if-bool (C.between 10 10 10) "yes" "no") "yes") then
    return P.tt

  define transparent main as computation
    bind _ <- test-between then
    return P.tt
end
