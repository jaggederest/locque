# Migrated from ../02_lambda.lq

import Prelude as P
import Assert as A

module LambdaTests contains

  define transparent id as value
    function x of-type String produce x

  define transparent apply-id as computation
    bind res <- id "hi" then
    bind _ <- perform io (A.assert-eq-string res "hi") then
    return P.tt

  define transparent main as computation
    bind _ <- apply-id then
    return P.tt
end
