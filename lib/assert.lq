import prelude as P

module assert contains
  define opaque assert-eq-nat as assert-eq-nat-prim
  define opaque assert-eq-string as assert-eq-string-prim
  define opaque assert-eq-bool as assert-eq-bool-prim

  define transparent assert-false as
    function msg String cond Boolean returns Unit value
      P::if-bool cond (error-prim msg) tt
    end
end
